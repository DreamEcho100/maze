{
	"name": "create-t3-turbo",
	"private": true,
	"engines": {
		"node": ">=23.7.0",
		"pnpm": "^10.7.1"
	},
	"packageManager": "pnpm@10.14.0",
	"scripts": {
		"build": "turbo run build",
		"clean": "git clean -xdf node_modules",
		"clean:workspaces": "turbo run clean",
		"db:push": "turbo -F @de100/db push",
		"db:studio": "turbo -F @de100/db studio",
		"dev": "turbo watch dev --continue",
		"dev:next": "turbo watch dev -F @de100/nextjs...",
		"format:check": "turbo run format:check --continue --",
		"format:fix": "turbo run format:fix --continue --",
		"lint": "turbo run lint --continue --",
		"lint:fix": "turbo run lint --continue -- --fix",
		"lint:ws": "pnpm dlx sherif@latest -p \"./playground/*\"",
		"typecheck": "turbo run typecheck",
		"ui-add": "turbo run ui-add",
		"postinstall": "pnpm lint:ws"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@turbo/gen": "^2.5.6",
		"tailwindcss": "catalog:",
		"turbo": "^2.5.6",
		"typescript": "catalog:"
	}
}
