{
	"name": "@de100/i18n",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"imports": {
		"#*": "./src/*"
	},
	"exports": {
		".": "./src/index.ts"
	},
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
