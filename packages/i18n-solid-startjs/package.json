{
	"name": "@de100/i18n-solid-startjs",
	"private": true,
	"version": "0.1.0",
	"imports": {
		"#*": "./src/*"
	},
	"type": "module",
	"exports": {
		"./client": {
			"types": "./dist/src/client/index.d.ts",
			"default": "./src/client/index.ts"
		},
		"./client/components/Link": {
			"types": "./dist/src/client/components/Link.d.ts",
			"default": "./src/client/components/Link.tsx"
		},
		"./server/init": {
			"types": "./dist/src/server/init.d.ts",
			"default": "./src/server/init.ts"
		}
	},
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"dev": "tsc",
		"format:fix": "biome format ./src --write",
		"format:check": "biome format ./src",
		"lint": "dotenv -v SKIP_ENV_VALIDATION=1 biome lint ./src",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@de100/biome-config": "workspace:*",
		"@de100/tsconfig": "workspace:*",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@de100/i18n": "workspace:*",
		"@de100/i18n-solidjs": "workspace:*",
		"@solidjs/meta": "catalog:",
		"@solidjs/router": "catalog:",
		"@solidjs/start": "catalog:",
		"solid-js": "catalog:",
		"vinxi": "catalog:"
	}
}
