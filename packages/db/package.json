{
	"name": "@de100/db",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"imports": {
		"#*": "./src/*"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.js",
			"require": "./src/index.js",
			"import": "./src/index.js"
		},
		"./auth/init": {
			"types": "./dist/auth/init.d.ts",
			"default": "./src/auth/init.js",
			"require": "./src/auth/init.js",
			"import": "./src/auth/init.js"
		},
		"./auth/def": {
			"types": "./dist/auth/def.d.ts",
			"default": "./src/auth/def.ts",
			"require": "./src/auth/def.ts",
			"import": "./src/auth/def.ts"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"default": "./src/client.js",
			"require": "./src/client.js",
			"import": "./src/client.js"
		},
		"./schema": {
			"types": "./dist/schema/index.d.ts",
			"default": "./src/schema/index.js",
			"require": "./src/schema/index.js",
			"import": "./src/schema/index.js"
		},
		"./src/client": {
			"types": "./dist/client.d.ts",
			"default": "./src/client.js"
		},
		"./src/schema/index": {
			"types": "./dist/schema/index.d.ts",
			"default": "./src/schema/index.js"
		},
		"./src/auth/init": {
			"types": "./dist/auth/init.d.ts",
			"default": "./src/auth/init.js"
		}
	},
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"dev": "tsc",
		"format": "biome format ./src ",
		"lint": "dotenv -v SKIP_ENV_VALIDATION=1 biome lint ./src",
		"push": "pnpm with-env drizzle-kit push",
		"studio": "pnpm with-env drizzle-kit studio",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false",
		"with-env": "dotenv -e ../../.env --",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate"
	},
	"dependencies": {
		"@de100/auth-core": "workspace:^",
		"drizzle-orm": "catalog:",
		"drizzle-zod": "catalog:",
		"id128": "catalog:",
		"pg": "catalog:",
		"ulid": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@de100/tsconfig": "workspace:*",
		"@types/pg": "catalog:",
		"dotenv-cli": "^8.0.0",
		"drizzle-kit": "catalog:",
		"typescript": "catalog:"
	}
}
